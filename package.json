        "resolve-url-loader": "5.0.0",
        "sass": "^1.23.0",
        "sass-loader": "^13.2.0",
        "tailwindcss": "^0.7.4",
        "vue": "^2.6.10",
        "object-path": "^0.11.8",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "sweetalert": "^2.1.2",
        "vue-clickaway": "^2.2.2",
        "vue-snotify": "^3.2.1"
    }
