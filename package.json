        "resolve-url-loader": "2.3.1",
        "sass": "^1.23.0",
        "sass-loader": "^7.3.1",
        "tailwindcss": "^0.7.4",
        "vue": "^2.6.10",
        "vue-template-compiler": "^2.6.10"
    },
    "dependencies": {
        "sweetalert": "^2.1.2",
        "vue-clickaway": "^2.2.2",
        "vue-snotify": "^3.2.1"
    }
